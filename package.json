{
  "name": "@saltcorn/docling",
  "version": "0.2.1",
  "description": "onvert document files (PDF, DOCX, PPTX) to Markdown",
  "main": "index.js",
  "dependencies": {
    "@saltcorn/data": "^0.9.0",
    "env-paths": "2.2.0",
    "tmp-promise": "3.0.3",
    "html-to-text": "9.0.5"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "repository": "github:saltcorn/docling",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": "off",
      "no-case-declarations": "off",
      "no-empty": "warn",
      "no-fallthrough": "warn"
    }
  }
}
